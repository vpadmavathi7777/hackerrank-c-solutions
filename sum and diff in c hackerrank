#include <stdio.h>
#include <string.h>
#include <math.h>
#include <stdlib.h>

int main()
{
	int a,b,sum,diff;
    float c,d,add,sub;
    scanf("%d%d",&a,&b);
    sum=a+b;
    diff=a-b;
    printf("%d %d\n",sum,diff);
    scanf("%f%f",&c,&d);
    add=c+d;
    sub=c-d;
    printf("%0.1f %0.1f\n",add,sub);
    return 0;
}
